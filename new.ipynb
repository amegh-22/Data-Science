    "import numpy as np\n",
    "a=np.random.randint(1,10,6)\n",
    "print(a)\n",
    "n=int(input(\"Enter the element to search\"))\n",
    "index=np.where(a==n)[0]\n",
    "if index.size>0:\n",
    "    print(\"found at\",index)\n",
    "else:\n",
    "    print(\"not found\")"
